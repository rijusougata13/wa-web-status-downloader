{
  "manifest_version": 3,
  "name": "whatsapp web status saver",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icons.png",
      "48": "icons/icons.png",
      "128": "icons/icons.png"
    }
  },
  "privacy_policy": "https://github.com/rijusougata13/wa-web-status-downloader/blob/main/privacy-policy.md"
}
